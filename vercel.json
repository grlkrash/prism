{
  "headers": [
    {
      "source": "/api/frame",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        },
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/frame",
      "destination": "/api/frame"
    }
  ],
  "functions": {
    "api/frame": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "public": true
} 